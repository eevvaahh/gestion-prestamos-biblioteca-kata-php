{
  "name": "evahe/gestion-prestamos-biblioteca-kata-php",
  "description": "Kata: Gestion prestamos biblioteca con testing en PHP",
  "type": "project",
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "evahe\\GestionPrestamosBiblioteca\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "evahe\\GestionPrestamosBiblioteca\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "phpunit --configuration phpunit.xml"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}